<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>{% block title %}BOURGIN Jérémy - blog de cuisine{% endblock %}</title>
	
	{% block stylesheets %}
		{{ encore_entry_link_tags('style') }}
	{% endblock %}
	
	{% block javascript %}
		{{ encore_entry_script_tags('script') }}
	{% endblock %}
</head>
<body>
<section id="header">
	<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
		<a class="navbar-brand" href="/">Navbar</a>
		<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		</button>
		<div class="collapse navbar-collapse" id="navbarNav">
			<div class="navbar-nav">
				<a href="/" class="nav-item nav-link">Accueil</a>
			</div>
		</div>
	</nav>
</section>

<section id="content" class="container-fluid row justify-content-md-center">
	<div class="col-xl-10 p-4">
		{% block body %}{% endblock %}
	</div>
</section>

<section id="footer">
</section>
</body>
</html>
