<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
		
        {% block stylesheets %}
		<link rel="stylesheet" type="text/css" href="{{ asset('lib/bootstrap.min.css') }}">
		<link rel="stylesheet" type="text/css" href="{{ asset('build/style.css') }}">		
		{% endblock %}
		
        {% block javascript %}
		<script type="text/javascript" src="{{ asset('lib/jquery.min.js') }}"></script>
		<script type="text/javascript" src="{{ asset('lib/bootstrap.min.js') }}"></script>
		<script type="text/javascript" src="{{ asset('build/script.js') }}"></script>
		{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
